{
  "rewrites": [
    {
      "source": "/admin",
      "destination": "https://mayaakademi-admin.vercel.app"
    },
    {
      "source": "/admin/:match*",
      "destination": "https://mayaakademi-admin.vercel.app/:match*"
    },
    {
      "source": "/site.webmanifest",
      "destination": "/admin/site.webmanifest"
    }
  ]
}